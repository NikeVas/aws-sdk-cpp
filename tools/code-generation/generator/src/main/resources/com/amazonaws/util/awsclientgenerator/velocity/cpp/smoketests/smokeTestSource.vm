class ${client}SmokeTestSuite : public Aws::Testing::AwsCppSdkGTestSuite {
};

#foreach($test in $tests)     
TEST_F(${client}SmokeTestSuite, $test.testcaseName )
{
    Aws::${client}::${client}ClientConfiguration& clientConfiguration;
